# Make the GKS emulator.

$checkout libgks.a lib$
$update	  libgks.a
$checkin  libgks.a lib$
$exit

libgks.a:
	gacwk.x		gks.com gks.h
	gca.x		gks.com gks.h <gset.h>
	gcas.x		gks.com gks.h <gset.h>
	gclks.x
	gclrwk.x	gks.com gks.h
	gclwk.x		gks.com gks.h
