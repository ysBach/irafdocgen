# ABGE -- Vector boolean greater than or equals.  C[i], type INT, is set to 1
# if A[i] is greater than B[i], else C[i] is set to zero.

procedure abge$t (a, b, c, npix)

PIXEL	a[ARB], b[ARB]
int	c[ARB]
int	npix
int	i

begin
	do i = 1, npix
	    $if (datatype == x)
	    if (abs (a[i]) >= abs (b[i]))
	    $else
	    if (a[i] >= b[i])
	    $endif
		c[i] = 1
	    else
		c[i] = 0
end
